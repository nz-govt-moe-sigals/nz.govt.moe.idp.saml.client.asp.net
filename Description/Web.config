      <add name="Redirect" type="nz.govt.moe.idp.saml.client.Actions.RedirectAction, nz.govt.moe.idp.saml.client"/>
    </Actions>
  </Federation>
  <SAML20Federation xmlns="urn:nz.govt.moe.idp.saml.client.configuration">
    <ShowError>true</ShowError>
    <ServiceProvider id="https://nz.govt.nz.idp.saml.client.demo.asp.net" server="https://localhost:44334/">
      <ServiceEndpoint localpath="/handlers/login.ashx" type="signon" redirectUrl="/Home/Secured" index="0"/>
      <ServiceEndpoint localpath="/handlers/logout.ashx" type="logout" redirectUrl="/Home/Index" index="1"/>
      <ServiceEndpoint localpath="/handlers/logout.ashx" type="soaplogout" errorBehaviour="throwexception"/>
      <ServiceEndpoint localpath="/handlers/metadata.ashx" type="metadata"/>
      <md:ContactPerson contactType="administrative" xmlns:md="urn:oasis:names:tc:SAML:2.0:metadata">
        <md:Company>MOE</md:Company>
        <md:GivenName>help</md:GivenName>
        <md:SurName>desk</md:SurName>
        <md:EmailAddress>helpdesk@moe.govt.nz</md:EmailAddress>
        <md:TelephoneNumber>4</md:TelephoneNumber>
      </md:ContactPerson>
    </ServiceProvider>
